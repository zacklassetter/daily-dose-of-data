ase all of the nodes of the tree besides the root will be on a single level, and they must all be stored in the queue.\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"br\",{}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"table\",{children:/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"tbody\",{children:[/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"tr\",{children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"th\",{children:\"Depth First Search\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"th\",{children:\"Complexity\"})]}),/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"tr\",{children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"td\",{children:\"Time Complexity\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"td\",{children:\"O(n)\"})]}),/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"tr\",{children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"td\",{children:\"Space Complexity\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"td\",{children:\"O(n)\"})]})]})})]});};/* harmony default export */ var BreadthFirstSearch_BreadthFirstSearchInformation = (BreadthFirstSearchInformation_BreadthFirstSearchInformation);\n// CONCATENATED MODULE: ./src/App.js\nvar App_App=function App(){//the daily visualization is stored as a string in a state variable\nvar _useState=Object(react[\"useState\"])(),_useState2=Object(slicedToArray[\"a\" /* default */])(_useState,2),visualization=_useState2[0],setVisualization=_useState2[1];//fetch the daily visualization as a string from the server\nObject(react[\"useEffect\"])(function(){fetch(\"/api\").then(function(res){return res.json();}).then(function(data){return setVisualization(data.visualization);});},[]);return/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(react_router_dom[\"a\" /* BrowserRouter */],{children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"header\",{children:\"DAILY DOSE OF DATA\"}),/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{className:\"main\",children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(src_Navigation_Navigation,{}),/*#__P